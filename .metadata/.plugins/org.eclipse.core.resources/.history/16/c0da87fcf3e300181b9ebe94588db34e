import java.net.*;

public class MyClient1 {
	
	public MyClient1() {
		try {
			// Socket() is for connecting some server with ip and port.
			Socket socket = new Socket("127.0.0.1",9999);
			
		} catch (UnknownHostException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}

	
}
